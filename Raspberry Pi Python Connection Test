import serial
import time


ser = serial.Serial('/dev/ttyACM0', 115200)

while True:
    ser.write(b"a\n")
    line= ser.readline().decode('utf-8').rstrip()
    print(line)
    time.sleep(0.5)
    ser.write(b"s\n")
    line= ser.readline().decode('utf-8').rstrip()
    print(line)
    time.sleep(0.5)
    ser.write(b"d\n")
    line= ser.readline().decode('utf-8').rstrip()
    print(line)
    time.sleep(0.5)
